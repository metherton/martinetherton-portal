{
  "entrypoint": "index.html",
  "shell": "src/portal-container.html",
  "fragments": [
    "src/portal-about.html",
    "src/portal-home.html",
    "src/portal-ons.html",
    "src/portal-ons-records-births.html",
    "src/portal-ons-home.html",
    "src/portal-ons-records.html",
    "src/portal-ons-records-about.html",
